{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Program with Nodemon",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "envFile": "${workspaceFolder}/.env",
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/nodemon",
        "program": "${workspaceFolder}/src/server.js"
      }
    ]
  }
  